{
  "name": "gerenciadordetarefas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s clean build public serve",
    "clean": "rm -rf dist",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "build": "babel . --delete-dir-on-start -d dist --copy-files --source-maps --ignore node_modules,dist,public,package.json,package-lock.json,yarn.lock",
    "dev": "cross-env NODE_ENV=development DEBUG=http:* nodemon index.js --exec babel-node",
    "public": "copyfiles -a public/**/*.* package.json dist/"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "express": "~4.16.1",
    "express-validator": "^4.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.1",
    "sequelize": "^6.21.3",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.11"
  }
}
